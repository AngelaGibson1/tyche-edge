# .env.example

# This file serves as a template for environment variables.
# Copy this file to .env.local and fill in the values.

# --- Client-Side Variables (accessible in browser) ---
NEXT_PUBLIC_APP_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NEXT_PUBLIC_SENTRY_DSN=

# --- Server-Side Variables (NOT accessible in browser) ---
DATABASE_URL=postgresql://username:password@localhost:5432/tyche_edge
SECRET_KEY=your-secret-key-minimum-16-characters
SPORTS_DATA_API_KEY=your-sports-data-api-key
ODDS_API_KEY=your-odds-api-key
JWT_SECRET=your-jwt-secret-for-authentication
